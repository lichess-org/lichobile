{
  "name": "lichobile",
  "version": "0.0.0",
  "description": "lichess.org mobile application",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/veloce/lichobile.git"
  },
  "keywords": [
    "lichess",
    "chess",
    "application",
    "mobile",
    "realtime",
    "android",
    "ios"
  ],
  "author": "Vincent Velociter (https://github.com/veloce)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/veloce/lichobile/issues"
  },
  "homepage": "https://github.com/veloce/lichobile",
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "~6.8.0",
    "babel-plugin-transform-react-require": "~1.0.1",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~2.0.0",
    "gulp-if": "~1.2.5",
    "gulp-minify-css": "~0.3.11",
    "gulp-preprocess": "~1.1.1",
    "gulp-rename": "~1.2.2",
    "gulp-streamify": "~0.0.5",
    "gulp-stylus": "~1.3.3",
    "gulp-util": "~3.0.1",
    "minimist": "~1.1.0",
    "ncp": "~2.0.0",
    "q": "~1.1.2",
    "stylus": "~0.49.1",
    "tsify": "2.0.1",
    "typescript": "~2.0.6",
    "vinyl-source-stream": "~1.0.0",
    "yargs": "~1.3.1"
  },
  "dependencies": {
    "babel-preset-es2015": "6.16.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "chart.js": "1.0.2",
    "chess.js": "git://github.com/veloce/chess.js#v0.9.2",
    "chessground-mobile": "veloce/chessground-mobile#8d4ffbb",
    "iscroll": "5.1.3",
    "lodash": "4.6.1",
    "mithril": "lhorie/mithril.js#9fbcea2",
    "moment": "2.14.1",
    "rlite-router": "1.1.3",
    "signals": "1.0.0",
    "watchify": "3.7.0",
    "whatwg-fetch": "1.0.0",
    "zanimo": "git://github.com/veloce/Zanimo"
  },
  "scripts": {
    "build": "gulp",
    "watch": "gulp watch",
    "build-js-dev": "gulp --target='cordova' --env='env-dev.json'",
    "build-js-prod": "gulp --mode='prod' --target='cordova' --env='env-prod.json'",
    "build-js-stage": "gulp --mode='prod' --target='cordova' --env='env-stage.json'",
    "android-dev": "npm run build-js-dev && cordova run android",
    "android-stage": "npm run build-js-stage && cordova run android",
    "android-prod": "npm run build-js-prod && cordova run android",
    "release-android": "npm run build-js-prod && cordova build android --release"
  }
}
